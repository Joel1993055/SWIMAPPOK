{
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write --list-different \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "build-check": "npm run type-check && npm run build",
    "test-build": "npm run build && echo 'âœ… Build successful'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "validate:rate-limits": "node scripts/validate-rate-limits.js",
    "validate:monitoring": "node scripts/validate-monitoring.js",
    "deploy:rate-limits": "bash scripts/deploy-rate-limiting.sh",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "commit": "cz",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:size": "npm run build && npx size-limit",
    "analyze:deps": "npx depcheck && npx npm-check-updates",
    "analyze:lighthouse": "npx lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "analyze:all": "npm run analyze:bundle && npm run analyze:deps",
    "bundle:check": "npm run build && npm run analyze"
  },
